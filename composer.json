{
    "name": "buzzingpixel/minify",
    "description": "Minify",
    "version": "1.1.0",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "TJ Draper",
            "email": "tj@buzzingpixel.com",
            "homepage": "https://buzzingpixel.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "buzzingpixel/corbomite-di": "^1.0",
        "mrclay/minify": "^3.0"
    },
    "require-dev": {
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "^7.5",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "buzzingpixel\\minify\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "buzzingpixel\\tests\\": "tests/"
        }
    },
    "extra": {
        "diConfigFilePath": "src/diConfig.php"
    },
    "config": {
        "bin-compat": "full",
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
